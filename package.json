{
  "name": "rndemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint --ext .tsx --ext .ts .",
    "start": "react-native start",
    "test": "jest",
    "format:check": "prettier --check \"**/**.{tsx,ts}\"",
    "format": "prettier \"**/**.{tsx,ts}\" --write",
    "prepare": "husky install",
    "start:dev": "NODE_ENV=development yarn start",
    "start:local": "NODE_ENV=local yarn start"
  },
  "dependencies": {
    "@brightlayer-ui/react-native-components": "^7.1.0",
    "@okta/okta-react-native": "^2.9.0",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.3",
    "add": "^2.0.6",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-modal": "^12.0.0",
    "react-native-paper": "^4.0.0",
    "react-native-safe-area-context": "^3.0.0",
    "react-native-screens": "^3.20.0",
    "react-native-vector-icons": "^8.0.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.0.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.24",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "^29.2.1",
    "cross-fetch": "^3.1.5",
    "detox": "^20.7.0",
    "eslint": "^8.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.2.0",
    "metro-react-native-babel-preset": "0.73.9",
    "msw": "^1.2.1",
    "prettier": "^2.8.7",
    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest/jestSetupFile.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/jest/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation/(.*))"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts --fix",
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  },
  "resolutions": {
    "@babel/core": "^7.20.0",
    "babel-loader": "^8.3.0"
  }
}
